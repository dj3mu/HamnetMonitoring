{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "server.urls": "http://*:3002",
  "ConnectionStrings": {
    "ResultDbConnectionString": "Filename=./Config/ResultDatabase.sqlite"
  },
  "DataAquisitionService": {
    "Enabled": true,
    "ConnectionStringFile": "/etc/HamnetMonitoringService/connectionstring.hamnetdb",
    "RefreshInterval": "0.00:04:30",
    "SnmpVersion": 2,
    "SnmpTimeoutSeconds": 5,
    "SnmpRetries": 3,
    "SubnetStartOffset": 0,
    "MaximumSubnetCount": 0,
    "TruncateFailingQueries": true,
    "MaximumParallelQueries": 10,
    "UseQueryCaching": true,
    "ExcludeFile": "/etc/HamnetMonitoringService/HamnetMonitorExcludedNets"
  },
  "MaintenanceSerivce": {
    "Enabled": true,
    "DryRun": false,
    "MaintenanceInterval": "0.02:48:03",
    "ResultsOutdatedAfter": "1.00:00:00",
    "CacheInvalidAfter": "1.00:00:00"
  }
}
