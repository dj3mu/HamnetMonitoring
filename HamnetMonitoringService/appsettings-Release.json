{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "server.urls": "http://*:3002",
  "ConnectionStrings": {
    "ResultDbConnection": "Filename=./Config/ResultDatabase.sqlite"
  },
  "DataAquisitionService": {
    "Enabled": true,
    "ConnectionStringFile": "/etc/HamnetMonitoringService/connectionstring.hamnetdb",
    "RefreshIntervalSecs": 621,
    "SnmpVersion": 2,
    "SnmpTimeoutSeconds": 5,
    "SnmpRetries": 3,
    "SubnetStartOffset": 0,
    "MaximumSubnetCount": 0,
    "TruncateFailingQueries": true,
    "MaximumParallelQueries": 10,
    "ExcludeFile": "/etc/HamnetMonitoringService/HamnetMonitorExcludedNets"
  },
  "MaintenanceSerivce": {
    "Enabled": true,
    "MaintenanceIntervalMins": 360
  }
}
